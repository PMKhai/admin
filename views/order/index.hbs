<div class="">
  <div class="page-title">
    <div class="title_left">
      <h3>Đơn đặt hàng</h3>
    </div>

    <div class="title_right">
      <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
        <div class="input-group">
          <input type="text" class="form-control" placeholder="Khách hàng...">
          <span class="input-group-btn">
            <button class="btn btn-default" type="button">Tìm!</button>
          </span>
        </div>
      </div>
    </div>
  </div>

  <div class="clearfix"></div>

  <div class="row">
    <div class="col-md-12">
      <div class="x_panel">

        <div class="x_content">


          <!-- start project list -->
          <table class="table table-striped projects">
            <thead>
              <tr>
                <th style="width: 1%">STT</th>
                <th style="width: 20%">Khách hàng</th>
                <th>Sản phẩm</th>
                <th>Trạng thái</th>
                <th style="width: 20%">Lựa chọn</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                {{#each order}}
                {{#each this}}
                <td>1</td>
                <td>
                  <a>{{this.info-order.name}}</a>
                  <br />
                  <small>Ngày đặt hàng {{this.date}}</small>
                </td>
                <td>
                  <ul class="list-inline">
                    {{#each this.productlist}}
                    <li>
                      <img src="{{this.image}}" class="avatar" alt="{{this.name}}">
                    </li>
                    {{/each}}
                  </ul>
                </td>
                <td>
                  <button type="button" class="btn {{this.display}} btn-xs">{{this.status}}</button>
                </td>
                <td>
                  <a href="/order/detail/{{this.index}}/{{@index}}" class="btn btn-primary btn-xs"><i
                      class="fa fa-folder"></i> Chi tiết </a>
                </td>
              </tr>
              {{/each}}
              {{/each}}
            </tbody>
          </table>
          <!-- end project list -->

        </div>
      </div>
    </div>
  </div>
</div>
{{#extend "namesFirst"}}
{{admin.username}}
{{/extend}}
{{#extend "namesSecond"}}
{{admin.username}}
{{/extend}}
{{#extend "idAccount"}}{{admin._id}}{{/extend}}
{{#if role}}
{{#extend "display-li"}}none{{/extend}}
{{/if}}